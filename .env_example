# DamaDam Login
DAMADAM_USERNAME=your_username
DAMADAM_PASSWORD=your_password

# Optional second account
DAMADAM_USERNAME_2=
DAMADAM_PASSWORD_2=

# Google Sheet URL
GOOGLE_SHEET_URL=https://docs.google.com/spreadsheets/d/xxxx/edit#gid=0

